module

struct S
    var a: int32
    var b: float32
end

function multS (a: S, b: S)() returns S
    return {a = a.a * b.a, b = a.b * b.b}


function addS (a: S, b: S)() returns S
    return {a = a.a + b.a, b = a.b + b.b}
end

activity accMultS (a: S, b: S) (acc: S)
    repeat
        acc = addS(acc, multS(a, b))
        await true
    end
end